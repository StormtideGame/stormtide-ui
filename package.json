{
  "name": "stormtide-ui",
  "version": "0.0.0-r1",
  "description": "UI for Stormtide",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r .nyc_output coverage debug production lib 2>/dev/null || true",
    "bundle": "gulp",
    "bundle-production": "NODE_ENV=production gulp",
    "lint": "eslint src test",
    "test": "npm run -s lint && flow && cross-env NODE_ENV=test nyc mocha --recursive",
    "coverage": "cross-env NODE_ENV=test nyc --reporter lcov mocha --recursive",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run -s clean && npm run -s bundle-production",
    "todo": "leasot {src,test}/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StormtideGame/stormtide-ui.git"
  },
  "author": "Lucien Greathouse <me@lpghatguy.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StormtideGame/stormtide-ui/issues"
  },
  "homepage": "https://github.com/StormtideGame/stormtide-ui#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.18.5",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "cssnano": "^3.9.1",
    "eslint": "^3.10.2",
    "eslint-plugin-flowtype": "^2.25.0",
    "fast-async": "^6.1.2",
    "flow-bin": "^0.37.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-watch": "^4.3.11",
    "leasot": "^4.2.1",
    "merge-stream": "^1.0.1",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "postcss-all-unset": "^1.1.0",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-sassy-import": "^1.2.3",
    "source-map-support": "^0.4.6",
    "webpack-stream": "^3.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "require": [
      "source-map-support/register",
      "babel-core/register"
    ]
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "stormtide-core": "git@github.com:StormtideGame/stormtide-core.git"
  }
}
